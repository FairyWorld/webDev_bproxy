    },
    "engines": {
        "node": ">= 8.0.0",
        "npm": ">= 5.5.0"
    },
    "preferGlobal": true,
    "homepage": "http://regx.vip/post/bproxy-doc/",
    "dependencies": {
        "chalk": "^2.4.2",
        "commander": "^2.20.3",
        "file-type": "^12.4.0",
